<Window x:Class="OverTop.ContentWindows.DockWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Icon="/Resources/Icons/icon.png"
        Style="{StaticResource FloatingWindowStyle}"
        Title="DockPanel" Height="60" Width="560"
        MouseLeftButtonDown="Window_MouseLeftButtonDown"
        MouseRightButtonDown="Window_MouseRightButtonDown"
        MouseDoubleClick="Window_MouseDoubleClick"
        Topmost="True" ShowInTaskbar="False"
        MouseEnter="Window_MouseEnter"
        MouseLeave="Window_MouseLeave"
        Drop="Window_Drop"
        KeyDown="Window_KeyDown" Loaded="Window_Loaded"
        MouseWheel="Window_MouseWheel">
    <StackPanel x:Name="ContentStackPanel"
                Orientation="Horizontal"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                ToolTip="右键 添加 快捷应用">
    </StackPanel>
</Window>
